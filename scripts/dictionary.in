def: make_dict
    lmake: dict
    lmake: keys
    lmake: values
    ladd: dict <- keys, values
    return: dict
enddef

def: add_value <- dict, key, value
    lget: keys = dict <- 0
    lget: values = dict <- 1
    ladd: keys <- key
    ladd: values <- value
enddef

def: get_value <- dict, key
    lget: keys = dict <- 0
    lget: values = dict <- 1
    lindex: index = keys <- key
    lget: value = values <- index
    return: value
enddef

