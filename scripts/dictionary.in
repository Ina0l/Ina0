def: make_dict
    lmake: dict
    lmake: keys
    lmake: values
    ladd: dict <- keys, values
    return: dict
enddef

def: set_value <- dict, key, value
    lget: keys = dict <- 0
    lget: values = dict <- 1
    if: not key in keys
        ladd: keys <- key
        ladd: values <- value
    endif
    if: key in keys
        lindex: index = keys <- key
        lset: values <- index = value
    endif
enddef

def: get_value <- dict, key
    lget: keys = dict <- 0
    lget: values = dict <- 1
    lindex: index = keys <- key
    lget: value = values <- index
    return: value
enddef

make_dict: test_dict
set_value: <- test_dict, "a", 1
set_value: <- test_dict, "b", "aaa"
set_value: <- test_dict, "c", True

get_value: value <- test_dict, "a"
out: value

